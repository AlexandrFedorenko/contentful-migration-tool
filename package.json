{
  "name": "contentful-migration-tool",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "setup": "node setup.js",
    "dev": "next dev",
    "watch": "next dev",
    "dev:docker": "WATCHPACK_POLLING=true CHOKIDAR_USEPOLLING=true next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "docker:build": "docker build -t contentful-migration-tool .",
    "docker:start": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up",
    "docker:dev:build": "docker-compose -f docker-compose.dev.yml up --build",
    "docker:dev:watch": "docker-compose -f docker-compose.dev.yml up --build",
    "docker:logs": "docker-compose -f docker-compose.dev.yml logs -f",
    "docker:clean": "docker-compose -f docker-compose.dev.yml down -v --remove-orphans",
    "docker:restart": "docker-compose -f docker-compose.dev.yml restart"
  },
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.13.0",
    "@types/node": "20.1.3",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "contentful-cli": "^2.6.31",
    "contentful-management": "^10.46.0",
    "cross-spawn": "^7.0.3",
    "dotenv": "^16.5.0",
    "eslint": "8.40.0",
    "eslint-config-next": "13.4.2",
    "next": "13.4.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "^6.0.0",
    "typescript": "5.0.4",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/cross-spawn": "^6.0.2",
    "@types/uuid": "^9.0.1"
  }
}
